                    "group": "navigation@1"
                },
                {
                    "command": "SQLTools.runFromBookmarks",
                    "when": "view == SQLTools/bookmarksExplorer && viewItem == bookmark.item",
                    "group": "inline@1"
                },
                {
                    "command": "SQLTools.editBookmark",
                    "when": "view == SQLTools/bookmarksExplorer && viewItem == bookmark.item",
                    "group": "inline@0"
                },
